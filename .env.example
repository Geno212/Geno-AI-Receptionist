PORT=3000
PUBLIC_URL=https://your-tunnel-subdomain.loca.lt

# Google Cloud (point to absolute path of JSON key)
GOOGLE_APPLICATION_CREDENTIALS=C:\\absolute\\path\\to\\gcp-service-account.json
GCP_PROJECT=your-gcp-project-id

# Choose LLM provider: openai or cloudflare
LLM_PROVIDER=cloudflare

# OpenAI-compatible (if LLM_PROVIDER=openai)
LLM_BASE_URL=https://api.openrouter.ai/v1
LLM_MODEL=meta-llama/llama-3.1-8b-instruct
LLM_API_KEY=sk-REPLACE

# Cloudflare Workers AI (if LLM_PROVIDER=cloudflare)
CF_ACCOUNT_ID=your-cf-account-id
CF_API_TOKEN=cf-api-token-with-ai-run-scope
# Recommended Egyptian-capable instruction model (@cf/meta/llama-3-8b-instruct or newer multilingual instruct model)
CF_MODEL=@cf/meta/llama-3-8b-instruct

# Twilio WebRTC Client
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_API_KEY_SID=SKxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_API_KEY_SECRET=your_api_key_secret
TWILIO_TWIML_APP_SID=APxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_CLIENT_IDENTITY=web-eg-test

# Logging
LOG_TRANSCRIPTS=1
